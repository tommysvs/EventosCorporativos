<Page x:Class="EventosCorporativos.Inicio"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:EventosCorporativos"
      mc:Ignorable="d" 
      d:DesignHeight="510" d:DesignWidth="1000"
      Title="Inicio">

    <Grid Background="#2E8A99">
        <Label x:Name="lvlEventosCorporativos" Content="EVENTOS CORPORATIVOS" HorizontalAlignment="Center" Margin="0,28,0,0" VerticalAlignment="Top" Foreground="White" FontSize="28" FontWeight="Bold"/>
        <Label x:Name="lvlIniciaSesion" Content="Por favor, inicia sesión antes de continuar." HorizontalAlignment="Center" VerticalAlignment="Center" Foreground="White" FontSize="14"/>
    </Grid>
</Page>
